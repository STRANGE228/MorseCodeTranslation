<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="13dip" />
            <stroke android:width="2dip" android:color="@color/stroke_check_btn" />
            <gradient android:angle="-90" android:startColor="@color/top_check_btn" android:endColor="@color/bottom_check_brn"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="13dip" />
            <stroke android:width="1dip" android:color="@color/stroke_check_btn" />
            <solid android:color="@color/top_check_btn"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="12dip" />
            <stroke android:width="1dip" android:color="@color/stroke_check_btn" />
            <gradient android:angle="-90" android:startColor="@color/top_check_btn" android:endColor="@color/bottom_check_brn" />
        </shape>
    </item>
</selector>